  "dependencies": {
    "@types/node": "^12.12.56",
    "@types/pg": "^7.14.4",
    "typescript": "^4.0.2",
    "bullmq": "^1.9.0",
    "sequelize": "^6.25.6",
    "pg": "^8.3.3",
